@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix MS2W:    <http://www.semanticweb.org/ontologies/2009/4/MS2W.owl#> .
@prefix xsd: 	 <http://www.w3.org/2001/XMLSchema#> .


MS2W:Site1
      a       MS2W:WikiSite ;
      MS2W:hasPage MS2W:lesson1 .

MS2W:Site2
      a       MS2W:WikiSite .
      

MS2W:lesson1
      a       owl:Thing , MS2W:SemanticWikiPage ;
      MS2W:head MS2W:Patch1 .

MS2W:Patch1
      a       MS2W:Patch ;
      MS2W:hasSite MS2W:Site1 ;
      MS2W:patchDate 0 ;
      MS2W:onPage MS2W:lesson1 .
      
MS2W:Patch2
      a       MS2W:Patch ;
      MS2W:hasSite MS2W:Site2 ;
      MS2W:onPage MS2W:lesson1 ;
      MS2W:patchDate 1 ;
      rdfs:subClassOf MS2W:Patch1 .


