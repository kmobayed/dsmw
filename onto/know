prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX MS2W: <http://www.semanticweb.org/ontologies/2009/4/MS2W.owl#> 
SELECT ?s1 ?s2 WHERE {{?p1 rdf:type MS2W:Patch}.
{?p2 rdf:type MS2W:Patch}.
{?s1 rdf:type MS2W:WikiSite}.
{?s2 rdf:type MS2W:WikiSite}. 
{?p1 MS2W:hasSite ?s1}.
{?p2 MS2W:hasSite ?s2}.
{?p1  rdfs:subClassOf ?p2}
}