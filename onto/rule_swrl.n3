@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl:    <http://www.w3.org/2003/11/swrl#> .
@prefix owl2xml:  <http://www.w3.org/2006/12/owl2-xml#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix swrlb:   <http://www.w3.org/2003/11/swrlb#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix MS2W:    <http://www.semanticweb.org/ontologies/2009/4/MS2W.owl#> .


MS2W:knows
      a       owl:ObjectProperty ;
      rdfs:domain MS2W:WikiSite ;
      rdfs:range MS2W:WikiSite ;
      rdfs:subPropertyOf owl:topObjectProperty .

MS2W:P1
      a       swrl:Variable .

MS2W:P2
      a       swrl:Variable .

MS2W:S1
      a       swrl:Variable .

MS2W:S2
      a       swrl:Variable .


[]    a       swrl:Imp ;
      swrl:body
              [ a       swrl:AtomList ;
                rdf:first
                        [ a       swrl:IndividualPropertyAtom ;
                          swrl:argument1 MS2W:P1 ;
                          swrl:argument2 MS2W:S1 ;
                          swrl:propertyPredicate
                                  MS2W:hasSite
                        ] ;
                rdf:rest
                        [ a       swrl:AtomList ;
                          rdf:first
                                  [ a       swrl:IndividualPropertyAtom ;
                                    swrl:argument1 MS2W:P2 ;
                                    swrl:argument2 MS2W:S2 ;
                                    swrl:propertyPredicate
                                            MS2W:hasSite
                                  ] ;
                          rdf:rest
                                  [ a       swrl:AtomList ;
                                    rdf:first
                                            [ a       swrl:IndividualPropertyAtom ;
                                              swrl:argument1 MS2W:P1 ;
                                              swrl:argument2 MS2W:P2 ;
                                              swrl:propertyPredicate
                                                      MS2W:previous
                                            ] ;
                                    rdf:rest ()
                                  ]
                        ]
              ] ;
      swrl:head
              [ a       swrl:AtomList ;
                rdf:first
                        [ a       swrl:IndividualPropertyAtom ;
                          swrl:argument1 MS2W:S1 ;
                          swrl:argument2 MS2W:S2 ;
                          swrl:propertyPredicate
                                  MS2W:knows
                        ] ;
                rdf:rest ()
              ] .

              
              
